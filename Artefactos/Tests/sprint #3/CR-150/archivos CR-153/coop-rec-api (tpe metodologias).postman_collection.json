				{
					"name": "/admin/material-aceptado",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"post material\", function () {",
									"    var r = pm.response.json();",
									"    if (r.ok != true) {",
									"        // pq falta get material by id",
									"        pm.sendRequest(\"localhost/tpe_metodologias/site/api/web/materiales-aceptados\", function (err, response) {",
									"            var material_posteado = pm.collectionVariables.get(\"material_posteado\");",
									"            var materiales = response.json().data.materialesAceptados;",
									"            var material_duplicado = materiales.find(function (x) {",
									"                return x.nombre == material_posteado.nombre;",
									"            })",
									"            pm.collectionVariables.set(\"material_posteado_id\", material_duplicado.data.id);",
									"        });",
									"    }",
									"    else {",
									"        pm.collectionVariables.set(\"material_posteado_id\", r.data.id);",
									"    }",
									"    //pm.expect(r.ok).to.eql(true);",
									"    pm.expect(pm.response.code).to.be.oneOf([200, 201, 202]);",
									"    ",
									"});"
								],
								"type": "text/javascript"
							}
						},
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"pm.collectionVariables.set(\"material_posteado\", JSON.parse(pm.request.body));"
								],
								"type": "text/javascript"
							}
						}
					],
